  "homepage": "https://github.com/AdrianMikk/joker#readme",
  "keywords": [
    "jokes",
    "app"
  ],
  "dependencies": {
    "adrian-package": "^0.0.6",
    "give-me-a-joke": "^0.5.1"
  },
